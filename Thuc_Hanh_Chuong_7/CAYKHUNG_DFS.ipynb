   "source": [
    "Bài 1. Tìm cây khung DFS"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "0db48aee",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "---- Code ----\n",
      "#include <iostream>\n",
      "#include <vector>\n",
      "#include <stack> \n",
      "#include <algorithm>\n",
      "#include <utility>\n",
      "\n",
      "using namespace std;\n",
      "\n",
      "\n",
      "int n, m;\n",
      "vector<vector<int>> adjlist;\n",
      "vector<bool> visited;\n",
      "stack<int> open;\n",
      "vector<int> parent;\n",
      "vector<pair<int, int>> tree;\n",
      "\n",
      "\n",
      "void DFS(int s) {\n",
      "    fill(visited.begin(), visited.end(), false);\n",
      "    fill(parent.begin(), parent.end(), -1);\n",
      "    while (!open.empty()) open.pop();\n",
      "    tree.clear();\n",
      "    open.push(s);\n",
      "    visited[s] = true;\n",
      "\n",
      "    \n",
      "    while (!open.empty()) {\n",
      "       \n",
      "        int u = open.top();\n",
      "        open.pop();\n",
      "\n",
      "        for (auto it = adjlist[u].rbegin(); it != adjlist[u].rend(); ++it) {\n",
      "            int v = *it;\n",
      "            if (!visited[v]) {\n",
      "                \n",
      "                parent[v] = u;\n",
      "                tree.push_back({ u, v });\n",
      "                visited[v] = true;\n",
      "                open.push(v);\n",
      "            }\n",
      "        }\n",
      "    }\n",
      "}\n",
      "\n",
      "void solve() {\n",
      "    \n",
      "    cin >> n >> m;\n",
      "    adjlist.resize(n + 1);\n",
      "    visited.resize(n + 1);\n",
      "    parent.resize(n + 1);\n",
      "\n",
      "    for (int i = 1; i <= m; i++) {\n",
      "        int u, v;\n",
      "        cin >> u >> v;\n",
      "        adjlist[u].push_back(v);\n",
      "        adjlist[v].push_back(u);\n",
      "    }\n",
      "\n",
      "    \n",
      "    for (int i = 1; i <= n; i++)\n",
      "        sort(adjlist[i].begin(), adjlist[i].end());\n",
      "\n",
      " \n",
      "    DFS(1);\n",
      "\n",
      "    cout << tree.size() << \"\\n\";\n",
      "    for (int i = 0; i < tree.size(); i++) {\n",
      "        cout << min(tree[i].first, tree[i].second) << \" \" << max(tree[i].first, tree[i].second) << \"\\n\";\n",
      "    }\n",
      "}\n",
      "\n",
      "int main() {\n",
      "    \n",
      "    solve();\n",
      "    return 0;\n",
      "}"
     ]
    }
   ],
